{% extends 'base.html.twig' %}

{% block body %}
    <h1>Trnvalidacion</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ trnValidacion.id }}</td>
            </tr>
            <tr>
                <th>Estadoverificado</th>
                <td>{% if trnValidacion.estadoVerificado %}Yes{% else %}No{% endif %}</td>
            </tr>
            <tr>
                <th>Fechamodificación</th>
                <td>{{ trnValidacion.fechaModificación }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('trnvalidacion_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('trnvalidacion_edit', { 'id': trnValidacion.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
