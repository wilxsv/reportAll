{% extends 'base.html.twig' %}

{% block body %}
    <h1>Catestados list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Estado</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for catEstado in catEstados %}
            <tr>
                <td><a href="{{ path('catestados_show', { 'id': catEstado.id }) }}">{{ catEstado.id }}</a></td>
                <td>{{ catEstado.estado }}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('catestados_show', { 'id': catEstado.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('catestados_edit', { 'id': catEstado.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('catestados_new') }}">Create a new catEstado</a>
        </li>
    </ul>
{% endblock %}
