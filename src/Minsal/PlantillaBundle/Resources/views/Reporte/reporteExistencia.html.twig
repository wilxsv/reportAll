{% extends 'base.html.twig' %}
{% block css %}
	{{ parent() }}
    <link href="{{ asset('assets/') }}plugins/bower_components/bootstrap-select/bootstrap-select.min.css" rel="stylesheet" />
{% endblock %}
{% block body %}
<!-- /.row -->
<div class="row">
    <div class="col-md-6">
        <div class="white-box">
            <h3 class="box-title m-b-0">Reporte de existencia</h3>
            <p class="text-muted m-b-30 font-13">Total de existencia</p>
            <div class="row">
                <form class="form-horizontal">
                    <div class="form-group">
                        <label for="inputEmail3" class="col-sm-3 control-label">Tipo de reporte</label>
                        <div class="col-sm-9">
                            <div class="form-horizontal  radio radio-success">
                                <input type="radio" name="radio" id="radio4" value="option4">
                                <label for="radio4">Por medicamento</label>
                            </div>
                            <div class="form-horizonta radio radio-info">
                                <input type="radio" name="radio" id="radio5" value="option5">
                                <label for="radio5">Por establecimiento</label>
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="inputEmail3" class="col-sm-3 control-label">Medicamento</label>
                        <div class="col-md-4">
                            <select class="selectpicker" data-style="form-control" placeholder="Todas las regiones">
                                <option>Mustard</option>
                                <option>Ketchup</option>
                                <option>Relish</option>
                            </select>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="inputEmail3" class="col-sm-3 control-label">Nivel de consulta:</label>
                        <div class="col-md-4">
                            <select class="selectpicker" data-style="form-control">
                                <option>Mustard</option>
                                <option>Ketchup</option>
                                <option>Relish</option>
                            </select>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="inputPassword3" class="col-sm-3 control-label">Region</label>
                        <div class="col-md-4">
                            <select class="selectpicker" data-style="form-control">
                                <option>Mustard</option>
                                <option>Ketchup</option>
                                <option>Relish</option>
                            </select>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="inputPassword4" class="col-sm-3 control-label">SIBASI</label>
                        <div class="col-md-4">
                            <select class="selectpicker" data-style="form-control">
                                <option>Mustard</option>
                                <option>Ketchup</option>
                                <option>Relish</option>
                            </select>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="inputPassword4" class="col-sm-3 control-label">Mes-Anio</label>
                        <div class="col-md-6">
                            <select class="selectpicker" data-style="form-control">
                                <option>Mustard</option>
                                <option>Ketchup</option>
                                <option>Relish</option>
                            </select>
                            <select class="selectpicker" data-style="form-control">
                                <option>Mustard</option>
                                <option>Ketchup</option>
                                <option>Relish</option>
                            </select>
                        </div>
                    </div>
                </div>
                <div class="form-group m-b-0">
                    <div class="col-sm-offset-3 col-sm-9">
                        <button type="submit" class="btn btn-info waves-effect waves-light m-t-10">Generar</button>
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>

{% endblock %}
{% block js %}
	{{ parent() }}
    <script src="{{ asset('assets/') }}plugins/bower_components/bootstrap-select/bootstrap-select.min.js" type="text/javascript"></script>
    <script src="{{ asset('assets/') }}sinab/js/custom.min.js"></script>
    <script src="{{ asset('assets/') }}sinab/js/jasny-bootstrap.js"></script>

{% endblock %}