{% set user = { username: app.session.get('username'), id: app.session.get('id'), correo: app.session.get('username'), nombre: app.session.get('name') } %}
{% if user is defined %}
<li class="dropdown">
  <a class="dropdown-toggle profile-pic" data-toggle="dropdown" href="javascript:void(0)">
    <b>
      {% if app.user %}{{ app.user.username }} {% else %} Invitado{% endif %}
    </b><span class="caret"></span> </a>
    <ul class="dropdown-menu dropdown-user">
      <li>
        <div class="dw-user-box">


          <div class="u-text">
            <h4>
              {% if app.user %}{{ app.user.username }} {% else %} Invitado{% endif %}
            </h4>
            <!-- correo y botón de perfil
            <p class="text-muted">varun@gmail.com</p>
            <a href="https://wrappixel.com/ampleadmin/ampleadmin-html/ampleadmin-horizontal-nav/profile.html" class="btn btn-rounded btn-danger btn-sm">View Profile</a>
          -->
        </div>
      </div>
    </li>
    <li role="separator" class="divider"></li>
    <li><a href="javascript:void(0)"><i class="ti-user"></i> Perfil</a></li>
    {% if is_granted('ROLE_ADMIN') %}

    <!-- enlaces propios de administración
    <li><a href="javascript:void(0)"><i class="ti-settings"></i> Configuración del Sistema</a></li>
  -->
  {% endif %}
  <li><a href="javascript:void(0)"><i class="fa fa-power-off"></i> Salir del sistema</a></li>
</ul>
<!-- /.dropdown-user -->
</li>
{% else %}
<li class="in"><a href="javascript:void(0)" ><b> Entrar al sistema</b></a></li>
{% endif %}
<!-- /.dropdown -->
</ul>
